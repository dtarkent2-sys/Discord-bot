{
  "name": "discord-ai-bot",
  "version": "1.0.0",
  "description": "Autonomous Discord AI bot with local AI models (Ollama), conversation memory, NLP processing, and self-learning capabilities",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "docker-build": "docker build -t discord-ai-bot .",
    "docker-run": "docker run -d --env-file .env --name discord-ai-bot discord-ai-bot"
  },
  "keywords": [
    "discord",
    "ai",
    "bot",
    "autonomous",
    "ollama",
    "nlp",
    "neural-network"
  ],
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "axios": "^1.7.9",
    "sqlite3": "^5.1.7",
    "node-schedule": "^2.1.1",
    "natural": "^8.0.1",
    "compromise": "^14.14.3",
    "brain.js": "^2.0.0-beta.23",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
